@model OrganisationCentrewiseJoiningCodeListViewModel
@{
    Layout = null;
    string controllerName = ViewContext.RouteData.Values["controller"].ToString();
    string actionName = ViewContext.RouteData.Values["action"].ToString();
    ModelPopUpViewModel sendDetailsModel = new ModelPopUpViewModel() { ModelPopUpId = "SendDetailsPopupId", ModalContentId = "SendDetailsContentId" };
}
<div class="row">
    @Html.Partial("~/Views/Shared/Datatable/_DatatableShowResult.cshtml", Model.PageListViewModel.RecordPerPage)
    @Html.Partial("~/Views/Shared/Datatable/_DatatableSearchData.cshtml", Model.PageListViewModel.SearchBy)
</div>
<div class="row">
    <div class="col-sm-12 table-responsive">
        <table id="datatable" class="action-set table table-striped table-bordered dt-responsive nowrap dataTable no-footer dtr-inline" style="border-collapse: collapse; border-spacing: 0px; width: 100%;" role="grid" aria-describedby="datatable_info">
            @Html.Partial("~/Views/Shared/Datatable/_DatatableColumns.cshtml", Model.PageListViewModel)
            <tbody>
                @if (Model?.OrganisationCentrewiseJoiningCodeList?.Count > 0)
                {
                    foreach (var item in Model?.OrganisationCentrewiseJoiningCodeList)
                    {
                        <tr>
                            <td>@item.JoiningCode</td>
                            <td>
                                @Html.Partial("~/Views/Shared/Datatable/_DatatableTrueFalse.cshtml", item.IsExpired)
                            </td>
                            <td class="text-center data-action">
                                @*  <a href="../@controllerName/SendDetails" title="Send" class="btn btn-sm btn-soft-success btn-circle"><i data-feather="send" style="width: 20px; height: 20px;"></i></a> *@
                                @if (item.IsExpired==false)
                                {

                                    <a data-bs-toggle="modal" data-bs-target="#@sendDetailsModel.ModelPopUpId" href="javascritp:void(0)" onclick="OrganisationCentrewiseJoiningCode.GetOrganisationCentrewiseJoiningCodeSend('@sendDetailsModel.ModalContentId','@item.JoiningCode')" title="Send" class="btn btn-sm btn-soft-success btn-circle"> <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-send"><line x1="22" y1="2" x2="11" y2="13"></line><polygon points="22 2 15 22 11 13 2 9 22 2"></polygon></svg></a>
                                }
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>
@Html.Partial("~/Views/Shared/Datatable/_DatatablePagination.cshtml", Model.PageListViewModel)
@Html.Partial("~/Views/Shared/_ModelPopUp.cshtml", sendDetailsModel)
<script src="~/js/coditech/Organisation/OrganisationCentrewiseJoiningCode.js"></script>

