@using Coditech.Admin.Utilities;
@model Dictionary<string, string>
@{
    Layout = null;
    long dBTMTraineeDetailId = Convert.ToInt32(Model?.FirstOrDefault(x => x.Key == "DBTMTraineeDetailId").Value);
    long personId = Convert.ToInt64(Model?.FirstOrDefault(x => x.Key == "PersonId").Value);
    string actionName = ViewContext.RouteData.Values["action"].ToString();
}
<div class="card sidebar-card">
    <div class="card-body">
        <div class="files-nav">
            <div class="nav flex-column nav-pills" id="files-tab" aria-orientation="vertical">
                <a class="nav-link" href="/DBTMTraineeDetails/UpdateDBTMTraineePersonalDetails?dBTMTraineeDetailId=@dBTMTraineeDetailId&personId=@personId" aria-selected="true">
                    <i class="ti-menu"></i><h5 class="m-0 ps-2">Personal Details</h5>
                </a>
                <a class="nav-link" href="/DBTMTraineeDetails/MemberOtherDetails?dBTMTraineeDetailId=@dBTMTraineeDetailId" aria-selected="true">
                    <i class="fas fa-info-circle"></i><h5 class="m-0 ps-2">Other Details</h5>
                </a>
                <a class="nav-link" href="/DBTMTraineeDetails/GetAssociatedTrainerList?SelectedParameter1=@dBTMTraineeDetailId&SelectedParameter2=@personId" aria-selected="true">
                    <i class="fas fa-id-card-alt"></i><h5 class="m-0 ps-2">Associated Trainer</h5>
                </a>
            </div>
        </div>
    </div>
</div>