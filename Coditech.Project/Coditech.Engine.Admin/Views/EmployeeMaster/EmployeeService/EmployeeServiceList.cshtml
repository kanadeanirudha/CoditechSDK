@using Coditech.Admin.Utilities;
@model EmployeeServiceListViewModel
@{
    ViewBag.Title = "Employee Service List";
    string controllerName = ViewContext.RouteData.Values["controller"].ToString();
    string actionName = ViewContext.RouteData.Values["action"].ToString();
    Dictionary<string, string> keyValuePairs = new Dictionary<string, string>();
    keyValuePairs.Add("EmployeeId", Model.EmployeeId.ToString());
    keyValuePairs.Add("PersonId", Model.PersonId.ToString());
}
<!-- Page Content-->
<div class="page-content mt-5">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-3 col-md-4 mb-3">
                @Html.Partial("~/Views/EmployeeMaster/_EmployeeDetailsSidePannel.cshtml", keyValuePairs)
            </div>

            <div class="col-lg-9 col-md-8">
                <div class="card">
                    <div class="card-header d-flex justify-content-between align-items-center flex-wrap">
                        <span class="card-title">Employee Service List : @Model.FirstName @Model.LastName</span>
                        @*
                        <a class="btn btn-sm btn-soft-primary"
                        href="/@controllerName/GetEmployeeService?employeeId=@Model.EmployeeId&personId=@Model.PersonId"
                        role="button">
                        <i class="fas fa-plus me-2"></i>Add Employee Service
                        </a>*@

                    </div>
                    <div class="card-body">
                        <div id="DataTablesDivId" class="dataTables_wrapper dt-bootstrap5 no-footer">
                            @Html.Partial("~/Views/EmployeeMaster/EmployeeService/_EmployeeServiceList.cshtml", Model)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end page content -->