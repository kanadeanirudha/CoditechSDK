@model OrganisationCentrewiseWhatsAppSettingViewModel
@{
    Layout = null;
}
@Html.HiddenFor(m => m.OrganisationCentrewiseWhatsAppSettingId)
<div class="col-lg-4">
    <div class="mb-3">
        @Html.LabelFor(x => x.WhatsAppPortalAccountId, new { @class = "form-label" })
